<osc server='v-m-fenice' port='3819' />
<vrpn>
  <tracker name="DTrack" host="v-vrpn">
    <transformation>
      <translate x="0.0" y="0.0" z="-1.6"/>
      <rotate x="1.0" y="1.0" z="1.0" angle="-2.0943951"/>
    </transformation>
    <sensor id="0" processor_method='tracker_1'/>
    <sensor id="1" processor_method='user_position' users='user B'>
      <transformation>
	<rotate x="1.0" y="1.0" z="1.0" angle="2.0943951"/>
      </transformation>
    </sensor>
    <sensor id="2" processor_method='user_position' users='user A'>
      <transformation>
	<rotate x="1.0" y="1.0" z="1.0" angle="2.0943951"/>
      </transformation>
    </sensor>
    <sensor id="3" processor_method='tracker_2'/>
  </tracker>
  <analog name="FlyStick" host="v-vrpn" processor_method="movements"/>
  <button name="FlyStick" host="v-vrpn" processor_method="buttons"/>
  <analog name="RumblePad" host="v-vrpn" processor_method="rum_a"/>
  <button name="RumblePad" host="v-vrpn" processor_method="rum_b"/>
  <text name="Speech" host="v-vrpn" processor_method="texts" users='user A'/>
</vrpn>
